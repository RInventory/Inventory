        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Jumlah/Stok"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
